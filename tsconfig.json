{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "reactAntdColumnResize": [
        "src"
      ],
      "reactAntdColumnResize/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumi/**/*",
    ".dumirc.ts",
    "src/**/*"
  ],
  "lib": [
    "es2015",
    "es2017",
    "ES2019",
    "dom"
  ]
}